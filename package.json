{
  "name": "use-access",
  "version": "0.0.1",
  "description": "a react hook and component for managing access.",
  "scripts": {
    "clear": "rm -fr build",
    "build": "rm -fr build&&parcel build",
    "check": "tsc --noEmit",
    "commit": "git-cz",
    "push-origin": "git push origin main",
    "publish": "npm publish --access public",
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint-examples": "eslint --ext .ts,.tsx ./examples",
    "release": "standard-version",
    "dev-server": "node ./dev-server/dev-server.js",
    "dev-web": "parcel ./dev-web/index.html",
    "example-web-useSubcribeExample": "parcel ./examples/useSubcribeExample/index.html",
    "example-server": "node ./examples/server.js"
  },
  "keywords": [
    "react",
    "hook",
    "javascript",
    "typescript"
  ],
  "type": "module",
  "main": "./build/index.js",
  "source": "index.ts",
  "types": "./build/index.d.ts",
  "module": "./build/index.js",
  "author": "catpotola",
  "license": "MIT",
  "devDependencies": {
    "@parcel/packager-ts": "2.9.2",
    "@parcel/transformer-typescript-types": "2.9.2",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-plugin-react": "^7.32.2",
    "git-cz": "^4.9.0",
    "json-server": "^0.17.3",
    "parcel": "^2.9.2",
    "process": "^0.11.10",
    "standard-version": "^9.5.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "files": [
    "build"
  ]
}
